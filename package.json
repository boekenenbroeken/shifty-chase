  "browserslist": "> 0.5%, last 2 versions, not dead",
  "resolutions": {
    "styled-components": "^5"
  },
  "alias": {
    "components": "./src/components",
    "types": "./src/types",
    "store": "./src/store",
    "styles": "./src/styles",
    "assets": "./src/assets"
