{
  "name": "shifty-chase",
  "version": "1.0.0",
  "source": "src/index.html",
  "repository": "https://github.com/boekenenbroeken/shifty-chase.git",
  "author": "boekenenbroeken",
  "license": "MIT",
  "scripts": {
    "start": "parcel",
    "clean-cache": "rm -rf .parcel-cache dist",
    "dev": "yarn clean-cache && yarn start",
    "build": "parcel build",
    "lint:js": "eslint . --ext .ts,.tsx",
    "prettify": "yarn prettier '**/*.(ts|tsx|css)' --write",
    "update:cssdb": "yarn bin browserslist --update-db"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "react": "^18",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@parcel/transformer-svg-react": "^2.6.1",
    "@parcel/transformer-typescript-tsc": "^2.6.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "typescript": "^4.7.4"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "alias": {
    "components": "./src/components",
    "config": "./src/config",
    "constants": "./src/constants",
    "pages": "./src/pages",
    "utils": "./src/utils",
    "types": "./src/types",
    "store": "./src/store",
    "api": "./src/api"
  }
}
