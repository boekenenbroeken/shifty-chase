    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.6.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.4"
